<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <title>Admin Panel</title>
    <!-- loader-->
    <link href="~/manage/css/pace.min.css" rel="stylesheet" />
    <script src="~/manage/js/pace.min.js"></script>
    <!--favicon-->
    @*<link rel="icon" href="~/manage/images/favicon.ico" type="image/x-icon">*@
    <!-- simplebar CSS-->
    <link href="~/manage/plugins/simplebar/css/simplebar.css" rel="stylesheet" />
    <!-- Bootstrap core CSS-->
    <link href="~/manage/css/bootstrap.min.css" rel="stylesheet" />
    <!-- animate CSS-->
    <link href="~/manage/css/animate.css" rel="stylesheet" type="text/css" />
    <!-- Icons CSS-->
    <link href="~/manage/css/icons.css" rel="stylesheet" type="text/css" />
    <!-- Sidebar CSS-->
    <link href="~/manage/css/sidebar-menu.css" rel="stylesheet" />
    <!-- Custom Style-->
    <link href="~/manage/css/app-style.css" rel="stylesheet" />
    @RenderSection("Style", required: false)

